{
  "entrypoint": "index.html",
  "shell": "src/my-app.html",
  "fragments": [
    "src/nc-nutrientsofinterest.html",
    "src/my-view2.html",
    "src/my-view3.html",
    "src/my-view404.html"
  ],
  "sources": [
    "src/**/*",
    "images/**/*",
    "bower.json",
    "node_modules/redux/**/*"
  ],
  "extraDependencies": [
    "manifest.json",
    "bower_components/webcomponentsjs/*.js",
    "my-service-worker.js"
  ],
  "lint": {
    "rules": ["polymer-2"]
  },
  "builds": [
     {
       "name": "unbundled",
       "bundle": false,
       "js": { "compile": true, "minify": true },
       "css": { "minify": true },
       "html": { "minify": true },
       "addServiceWorker": false,
       "insertPrefetchLinks": false
     },
     {
       "name": "bundled",
       "bundle": true,
       "js": { "compile": true, "minify": true },
       "css": { "minify": true },
       "html": { "minify": true },
       "addServiceWorker": false,
       "insertPrefetchLinks": false
     }
   ]
}
